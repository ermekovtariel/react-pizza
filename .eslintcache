ct\";\nimport { useDispatch, useSelector } from \"react-redux\";\n\nimport { Categories, SortPopup, PizzaBlock, PizzaLoadingBlock } from \"../components\";\n\nimport { setCategory, setSortBy } from \"../redux/actions/filters\";\nimport { fetchPizzas } from \"../redux/actions/pizzas\";\nimport { addPizzaToCart } from \"../redux/actions/cart\";\n\nconst categoryNames = [\n  \"мясные\",\n  \"Вегетарианская\",\n  \"Гриль\",\n  \"Острые\",\n  \"Закрытые\",\n];\nconst sortItems = [\n  { name: \"популярности\", type: \"popular\", order: 'desc' },\n  { name: \"цене\", type: \"price\", order: 'desc' },\n  { name: \"алфавиту\", type: \"name\", order: 'asc' },\n];\n\nfunction Home() {\n  const dispatch = useDispatch();\n  const items = useSelector(({ pizzas }) => pizzas.items);\n  const isLoaded = useSelector(({ pizzas }) => pizzas.isLoaded);\n  const { category, sortBy } = useSelector(({ filters }) => filters);\n\n  React.useEffect(() => {\n    //Remove to redux and connect redux-thunk\n    //Looking for filter and sort, and put URL parametrs from redux\n    //Make a imitation loading pizzas like a skeleton(what have in css)   \n    dispatch(fetchPizzas(sortBy, category))\n  }, [category, sortBy]);\n\n  const onSelectCategory = useCallback((idx) => {\n    dispatch(setCategory(idx));\n  }, []);\n  \n  const onSelectSortType = useCallback((type) => {\n    dispatch(setSortBy(type));\n  }, []);\n\n  const  handleAddPizzaToCart = obj =>{\n    dispatch({\n      type: 'ADD_PIZZA_CART',\n      payload: obj,\n    })\n  }\n\n  return (\n    <div className=\"container\">\n      <div className=\"content__top\">\n        <Categories \n          activeCategoryIndex={category} \n          onClickCategory={onSelectCategory} \n          items={categoryNames} />\n        <SortPopup \n          activeSortType={sortBy.type} \n          items={sortItems} \n          onClickSortType={onSelectSortType}/>\n      </div>\n      <h2 className=\"content__title\">Все пиццы</h2>\n      <div className=\"content__items\">\n        {isLoaded \n        ? items.map((obj) => \n            <PizzaBlock \n              onClickAddPizza={handleAddPizzaToCart}\n              key={obj.id} \n              isLoading={true}\n              {...obj} />)\n        : Array(12).fill(0).map((_,idx)=><PizzaLoadingBlock key={idx} />)}\n      </div>\n    </div>\n  );\n}\n\nexport default Home;\n","/home/admin-i/codeZ/react-pizza/src/pages/index.js",[],"/home/admin-i/codeZ/react-pizza/src/pages/Cart.jsx",[],"/home/admin-i/codeZ/react-pizza/src/redux/store.js",[],"/home/admin-i/codeZ/react-pizza/src/redux/reducers/filters.js",[],"/home/admin-i/codeZ/react-pizza/src/redux/reducers/pizzas.js",[],"/home/admin-i/codeZ/react-pizza/src/redux/reducers/index.js",[],"/home/admin-i/codeZ/react-pizza/src/redux/actions/pizzas.js",[],"/home/admin-i/codeZ/react-pizza/src/redux/actions/filters.js",[],"/home/admin-i/codeZ/react-pizza/src/components/PizzaBlock/LoadingBlock.jsx",[],"/home/admin-i/codeZ/react-pizza/src/components/PizzaBlock/index.jsx",[],"/home/admin-i/codeZ/react-pizza/src/redux/reducers/cart.js",[],"/home/admin-i/codeZ/react-pizza/src/redux/actions/cart.js",["90"],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":8,"column":10,"nodeType":"97","messageId":"98","endLine":8,"endColumn":24},{"ruleId":"99","severity":1,"message":"100","line":34,"column":6,"nodeType":"101","endLine":34,"endColumn":24,"suggestions":"102"},{"ruleId":"99","severity":1,"message":"103","line":38,"column":6,"nodeType":"101","endLine":38,"endColumn":8,"suggestions":"104"},{"ruleId":"99","severity":1,"message":"103","line":42,"column":6,"nodeType":"101","endLine":42,"endColumn":8,"suggestions":"105"},{"ruleId":"95","severity":1,"message":"106","line":1,"column":8,"nodeType":"97","messageId":"98","endLine":1,"endColumn":14},"no-native-reassign",["107"],"no-negated-in-lhs",["108"],"no-unused-vars","'addPizzaToCart' is defined but never used.","Identifier","un